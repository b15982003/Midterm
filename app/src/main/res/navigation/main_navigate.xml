<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/main_navigate"
    app:startDestination="@id/mainActivity"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedNavigation">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.ray.schoolmidterm.MainActivity"
        android:label="MainActivity"
        tools:layout="@layout/activity_main"
        />
    <dialog
        android:id="@+id/publishDialog"
        android:name="com.ray.schoolmidterm.publishDialog.PublishDialog"
        android:label="PublishDialog"
        tools:layout="@layout/fragment_publish_dialog"
        />
    <action android:id="@+id/action_global_publishDialog" app:destination="@id/publishDialog"/>
</navigation>